	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"postinstall": "ts-node ./scripts/install-config.ts",
		"dev": "concurrently -p --names=\"Server build,Frontend app,Server app\" --prefix-colors=\"yellow,red,green\" \"pnpm run build:watch\" \"pnpm run serve:frontend\" \"pnpm run serve:backend\"\n",
		"serve:frontend": "nx run cuentoneta:serve:development",
		"serve:backend": "node -r ts-node/register --watch-path=./dist/cuentoneta/server --env-file=.env dist/cuentoneta/server/server.mjs",
		"build": "nx run cuentoneta:build:production --skip-nx-cache",
		"build:watch": "nx run cuentoneta:build:production --skip-nx-cache --watch",
		"config:dev": "node -r ts-node/register --env-file=.env ./scripts/set-environment.ts",
		"config:production": "ts-node scripts/set-environment.ts",
		"storybook": "nx run cuentoneta:storybook",
		"storybook:build": "nx run cuentoneta:build-storybook",
		"test": "nx test",
		"test:watch": "nx test --watch",
		"sanity:dev": "cd cms && sanity dev",
		"prepare": "husky"
	},
	"private": true,
