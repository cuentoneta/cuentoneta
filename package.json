	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"postinstall": "pnpm run config",
		"dev": "nx run cuentoneta:serve:development",
		"build": "nx run cuentoneta:build:production --skip-nx-cache",
		"config": "node -r ts-node/register ./scripts/set-environment.ts",
		"delete-unused-assets": "node -r ts-node/register --env-file=.env ./scripts/delete-unused-assets.ts",
		"storybook": "nx run cuentoneta:storybook",
		"storybook:build": "nx run cuentoneta:build-storybook",
		"lint": "npx nx lint --skip-nx-cache",
		"stylelint": "npx nx stylelint cuentoneta --skip-nx-cache",
		"test": "nx test --skip-nx-cache",
		"test:watch": "nx test --watch",
		"test:e2e": "nx run cuentoneta:e2e",
		"sanity:dev": "cd cms && sanity dev",
		"sanity:extract-schema": "cd cms && npx sanity schema extract --enforce-required-fields --workspace=production-workspace",
		"sanity:run-typegen-generator": "cd cms && npx sanity typegen generate",
		"prepare": "husky"
	},
	"private": true,
